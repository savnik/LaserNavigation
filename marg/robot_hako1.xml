<robot>
    <name>HAKO1</name>
    <module>
        <name>keeper1</name>
        <host>hako1</host>
        <port>24926</port>
        <autoConnect>true</autoConnect>
    </module>
    <controlButtons>
        <start moduleName="nav" command="var global.mission.start='1'"/>
        <pause moduleName="nav" command="var global.drive.pause='1'"/>
        <stop moduleName="nav" command="var global.mission.stop='0'"/>
    </controlButtons>
</robot>
